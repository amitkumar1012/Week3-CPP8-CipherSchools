#include<bits/stdc++.h>;
using namespace std;

int main()
{
    list<int>1;
    1.push_back(10);
    1.push_back(20);
    1.push_back(30);
    list<int>::iterator it = 1.begin();
    for(;it!=1.end();it++)
    {

    }
    cout<<endl;
    1.push_front(120);
    1.pop_back();
    1.pop_front();
    for(;it!=1.end();it++)
    {
        cout<<*it<<"";
    }

    return 0;

}
